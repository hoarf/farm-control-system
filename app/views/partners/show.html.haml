- title "Detalhes do Contato"
.row
  .col-md-4
    .panel.panel-default
      .panel-heading= "Dados sobre #{@partner.name}"
      .panel-body
        %p
          %b Nome:
          = @partner.name
        %p
          %b Contato:
          = @partner.contact
      .panel-footer
        = link_to 'Editar', edit_partner_path(@partner)
        = link_to 'Voltar', partners_path
