<!DOCTYPE html>
%html
  = render "shared/head"
  %body
    #wrapper
      = render "shared/navigation"
      #page-wrapper
        = render "shared/title"
        .row
          .col-md-12
            %table{ data: { source: request.path }, id: table_id, class: table}
              %thead
                %tr
                  = yield
              %tbody
      = render "shared/footer"
